//Números Correlativos 1 a N

#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Ingresa un número entero (N): ";
    cin >> n;
    
    cout << "Números correlativos entre 1 y " << n << ":" << endl;
    
    for (int i = 1; i <= n; ++i) {
        cout << i << " ";
    }
    
    cout << endl;
    return 0;
}
